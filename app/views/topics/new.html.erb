<%= params.inspect %>

<%= form_for @topic do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.hidden_field :board_id, value: params[:board] %>
  <br>
  <%= form.label :user_id %>
  <%= form.text_field :user_id %>
  <br>
  <%= form.fields_for @first_post do |first_post| %>
    <%= first_post.text_field :content %>
  <% end %>
  <br>
  <%= form.submit %>
<% end %>